{
  "expo": {
    "name": "GliControl",
    "slug": "GliControl",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#700700"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.GliControl"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.GliControl"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "23d7d37a-c04d-4821-a623-b8e78b46ce9d"
      }
    }
  }
}
